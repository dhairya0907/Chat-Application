{
  "expo": {
    "name": "Chat Application",
    "slug": "Chat-Application",
    "privacy": "unlisted",
    "description": "Simple Chat Application build using React Native and Firebase.",
    "version": "1.0.0",
    "owner" : "dhairya",
    "orientation": "portrait",
    "sdkVersion" : "42.0.0",
    "githubUrl": "https://github.com/dhairya0907/Chat-Application",
    
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":  "com.dhairya0907.chatappliction"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dhairya0907.chatappliction"
    }
  }
}
